{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "panel",
  "name": "Psychrometric Chart",
  "id": "ventura-psychrometric-panel",
  "info": {
    "description": "View air conditions on a psychrometric chart.",
    "author": {
      "name": "Nicolas Ventura",
      "url": "https://www.nersc.gov"
    },
    "keywords": [
      "air",
      "temperature",
      "humidity"
    ],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://nicfv.github.io/Psychart"
      },
      {
        "name": "GitHub",
        "url": "https://github.com/nicfv/Psychart"
      },
      {
        "name": "License",
        "url": "https://github.com/nicfv/Psychart/blob/main/LICENSE"
      }
    ],
    "screenshots": [
      {
        "name": "HVAC use case: Air handler supply temperatures.",
        "path": "img/screenshot_ahu.png"
      },
      {
        "name": "IT use case: Inlet conditions of an air-cooled compute cabinet.",
        "path": "img/screenshot_cabinet.png"
      },
      {
        "name": "Meteorological use case: Long-term outdoor air conditions.",
        "path": "img/screenshot_out.png"
      },
      {
        "name": "General use case: Psychart as a thermodynamic calculator.",
        "path": "img/screenshot_calc.png"
      },
      {
        "name": "Random data, for illustrative purposes on customizing the appearance.",
        "path": "img/screenshot_rand.png"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
